{
    "name": "@dashdot/router",
    "version": "0.7.0",
    "description": "A simple and functional native Node router with some helpers.",
    "license": "MIT",
    "files": [
        "dist/"
    ],
    "main": "./dist/cjs/index.js",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "scripts": {
        "lint": "eslint .",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "codecov": "codecov",
        "build": "rm -rf dist/* && tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json && ./fixup",
        "prepare": "npm run build"
    },
    "dependencies": {
        "url-pattern": "^1.0.3"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.16.10",
        "@babel/preset-env": "^7.16.11",
        "@dashdot/eslint-config-base": "^0.3.0",
        "codecov": "^3.8.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.0.0",
        "got": "^11.8.2",
        "jest": "^27.4.7",
        "test-listen": "^1.1.0",
        "typescript": "^4.5.5"
    },
    "eslintConfig": {
        "extends": "@dashdot/eslint-config-base"
    }
}
